{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "build": "npx tsc",
    "dev": "npm run build && concurrently \"npx tsc --watch\" \"nodemon --watch .build --delay 2.5 ./bin/www\"",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "migration:gen": "sequelize migration:create --migrations-path ./sequelize/migrations/ --name",
    "migration:run": "sequelize db:migrate --migrations-path sequelize/migrations"
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.511.0",
    "@aws-sdk/client-s3": "^3.540.0",
    "@aws-sdk/client-sts": "^3.454.0",
    "@aws-sdk/lib-storage": "^3.540.0",
    "@aws-sdk/s3-request-presigner": "^3.456.0",
    "@hookform/resolvers": "^5.0.1",
    "@repo/backend": "*",
    "@types/uuid": "^9.0.8",
    "bcryptjs": "^3.0.2",
    "body-parser": "^1.20.2",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "http-errors": "^1.8.0",
    "mongodb": "^6.1.0",
    "mongoose": "^8.0.3",
    "morgan": "^1.10.0",
    "mysql2": "^3.14.0",
    "prettier": "^3.1.0",
    "react-hook-form": "^7.55.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "uuid": "^9.0.1",
    "winston": "^3.13.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@repo/types": "*",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.20",
    "@types/node": "^20.8.9",
    "@types/swagger-jsdoc": "^6.0.4",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.9",
    "typescript": "^5.2.2",
    "typescript-json-schema": "^0.63.0"
  }
}
